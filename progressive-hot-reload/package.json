    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.0.1",
    "express": "~3.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^5.0.5",
